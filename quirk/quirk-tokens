//duples of token:lexeme

//keywords
VAR:var
FUNCTION:function
RETURN:return
PRINT:print

//operators
ASSIGN:=
ADD:+
SUB:-
MULT:*
DIV:/
EXP:^

//Separators
LPAREN:(
RPAREN:)
LBRACE:{
RBRACE:}
COMMA:,
COLON::

//literals
NUMBER:((\d+(\.\d*)?)|(\.\d+))
IDENT: [a-zA-Z]+[a-zA-Z0-9_]*

//removed due to ambiguity! INDEX: [0-9]+
